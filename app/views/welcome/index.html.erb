<%= link_to 'New Article', new_article_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Actions</th>
  </tr>

  <% @articles.each do |article| %>
    <tr id= "<%= dom_id(article) %>">
      <td><%= article.title %></td>
      <td><%= article.content %></td>
      <td><%= link_to 'Show', article_path(article) %>
      <%= link_to 'Edit', edit_article_path(article) %></td>
    </tr>
  <% end %>
</table>